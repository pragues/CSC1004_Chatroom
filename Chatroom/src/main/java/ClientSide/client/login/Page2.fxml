<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.stage.*?>

<Stage xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientSide.client.login.Page2">
   <scene>
      <Scene>
         <root>
            <AnchorPane prefHeight="400.0" prefWidth="600.0">
               <children>
                  <Label layoutX="117.0" layoutY="112.0" prefHeight="44.0" prefWidth="107.0" text="USERNAME">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <Label layoutX="117.0" layoutY="170.0" prefHeight="44.0" prefWidth="107.0" text="PASSWORD">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <Label layoutX="49.0" layoutY="55.0" prefHeight="44.0" prefWidth="174.0" text="Please enter your :">
